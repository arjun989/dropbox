<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Root</servlet-name>
		<servlet-class>com.arjun.assignment3.RootServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Root</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpAndDownServlet</servlet-name>
		<servlet-class>com.arjun.assignment3.UpAndDownServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpAndDownServlet</servlet-name>
		<url-pattern>/upanddown</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addFolder</servlet-name>
		<servlet-class>com.arjun.assignment3.addFolder</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addFolder</servlet-name>
		<url-pattern>/addDir</url-pattern>
	</servlet-mapping>
	
</web-app>
